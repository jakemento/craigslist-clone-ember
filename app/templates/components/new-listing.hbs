{{#if addNewListing}}
  <h1>New Listing</h1>
  <div>
   <form>
     <button {{action 'listingFormHide'}}> Cancel </button>

     <div class="form-group">
       <label for="image">image</label>
       {{input value=image id="image"}}
     </div>

     <div class="form-group">
       <label for="text">text</label>
       {{input value=text id="text"}}
     </div>

     <div class="form-group">
       <label for="title">title</label>
       {{input value=title id="title"}}
     </div>

     <div class="form-group">
       <label for="address">address</label>
       {{input value=address id="address"}}
     </div>

     <div class="form-group">
       <label for="date">date</label>
       {{input value=date id="date"}}
     </div>

     <div class="form-group">
       <label for="zip">zip</label>
       {{input value=zip id="zip"}}
     </div>

     <div class="form-group">
       <label for="author">author</label>
       {{input value=author id="author"}}
     </div>

     <button {{action 'save1'}}>Save</button>
   </form>
  </div>
  {{else}}
  <button {{action 'listingFormShow'}}>New Listing</button>
{{/if}}
