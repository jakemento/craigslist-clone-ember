<center> <h1> Craig's List </h1> </center>


<center> <ul>
  {{#each model.categories as |category|}}
    <div class="col-md-6">
      {{category-tile category=category}}
    <ul>
      {{#each model.subcategories as |subcategory|}}
      <li> {{#link-to 'subcategory' subcategory.id}} {{subcategory.type}} {{/link-to}}
      </li>  {{/each}} <br>
    </ul> <hr>
  </div>
  {{/each}}
</ul> </center>
<hr>

{{new-listing save2="save3"}}

<ul>
  {{#each model.listings as |listing|}}
    {{listing-tile listing=listing deleteListing="deleteListing"}}
  {{/each}}
</ul>
